  }, {
    key: 'deletePost',
    value: function deletePost() {
      var deletePassKey = this.state.passKey;
      firebase.database().ref('posts/' + this.state.group + '/posts/' + deletePassKey).remove();
    }
  }, {
    key: 'notifyPeople',
    value: function notifyPeople() {
      var addedUsers = [];
      var firebaseItems = this.state.firebaseItems;
      for (i = 0; i < this.state.firebaseItems.length; i++) {
        if (_names.expoID != firebaseItems[i].userID && this.state.passUserID != firebaseItems[i].userID) {
          var uploadID = true;
          for (k = 0; k < addedUsers.length; k++) {
            if (this.state.firebaseItems[i].userID == addedUsers[k]) {
              var uploadID = false;
              break;
            }
          }
          if (uploadID) {
            firebase.database().ref('users/' + this.state.firebaseItems[i].userID + '/notifications').push().set({
              time: this.state.time,
              group: this.state.group,
              okey: this.state.passKey,
              content: this.state.commentInput,
              color: '#ffc7a5',
              postContent: this.state.passContent,
              postUserExpoID: this.state.passUserID,
              postUserNameID: this.state.passNameID,
              nameNumber: _names.expoNameIDConst * 2 + (this.state.passContent && Math.floor(this.state.passContent.length / 10))
            });
            addedUsers.push(this.state.firebaseItems[i].userID);
          }
        }
      }
    }
  }, {
    key: 'render',
    value: function render() {
      var _this14 = this;

      var params = this.props.navigation.state.params;


      return _react2.default.createElement(
        _reactNative.View,
        { style: { flex: 10000, backgroundColor: '#e8e8e8' } },
        this.state.fontLoaded ? _react2.default.createElement(
          _reactNative.View,
          { style: { flex: 10000, backgroundColor: '#e8e8e8' } },
          _react2.default.createElement(_reactNative.View, { style: { width: 2500, height: 3, backgroundColor: '#f4f4f4' } }),
          _react2.default.createElement(
            _reactNative.View,
            { style: { width: this.state.postWidth, height: 40, backgroundColor: '#f4f4f4', flexDirection: 'row', justifyContent: 'center' } },
            _react2.default.createElement(_reactNative.TextInput, {
              style: { height: 40, width: 260, borderRadius: 7, paddingRight: 7, paddingLeft: 7, backgroundColor: '#D3D3D3' },
              onChangeText: function onChangeText(commentInput) {
                return _this14.setState({ commentInput: commentInput });
              },
              value: this.state.commentInput,
              autoCorrect: true,
              underlineColorAndroid: '#D3D3D3',
              autoCapitalize: 'sentences',
              placeholder: "Leave a comment",
              placeholderTextColor: '#696969'
            }),
            _react2.default.createElement(_reactNative.View, { style: { width: 5, height: 3, backgroundColor: '#f4f4f4' } }),
            _react2.default.createElement(_reactNative.Button, {
              style: { justifyContent: 'center' },
              onPress: function onPress() {
                _reactNative.Keyboard.dismiss();
                _this14.getItems();
                if (_this14.state.commentInput.length > 0) {
                  if (_this14.state.replying == false) {
                    firebase.database().ref('posts/' + _this14.state.group + '/posts/' + _this14.state.passKey + '/commentlist').push().set({ comment: _this14.state.commentInput, userNameId: _names.expoNameIDConst, userID: _names.expoID, likes: 0, dislikes: 0, time: Date.now(), replies: false });
                  }
                  if (_this14.state.replying == true) {
                    firebase.database().ref('posts/' + _this14.state.group + '/posts/' + _this14.state.passKey + '/commentlist/' + _this14.state.replyingToKey + '/replylist').push().set({ comment: _this14.state.commentInput, userNameId: _names.expoNameIDConst, userID: _names.expoID, likes: 0, dislikes: 0, time: Date.now(), parentKey: _this14.state.replyingToKey });
                    firebase.database().ref('posts/' + _this14.state.group + '/posts/' + _this14.state.passKey + '/commentlist/' + _this14.state.replyingToKey).update({
                      replies: true
                    });
                    _this14.setState({ replying: false });
                  }
                  if (_names.expoID != _this14.state.passUserID) {
                    firebase.database().ref('users/' + _this14.state.passUserID + '/notifications').push().set({
                      time: _this14.state.time,
                      group: _this14.state.group,
                      okey: _this14.state.passKey,
                      content: _this14.state.commentInput,
                      color: '#ffc7a5',
                      postContent: params && params.postContent,
