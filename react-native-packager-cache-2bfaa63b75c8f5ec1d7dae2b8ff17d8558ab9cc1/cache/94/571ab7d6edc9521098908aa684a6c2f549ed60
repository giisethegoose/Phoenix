    key: 'pressDislike',
    value: function pressDislike(key, likes, dislikes) {
      var _this10 = this;

      var usersLiked = [];
      var query = firebase.database().ref('posts/' + this.state.group + '/posts/' + this.state.passKey + '/commentlist/' + key + '/usersVoted').orderByKey();
      query.once('value', function (snap) {
        snap.forEach(function (child) {
          usersLiked.push({
            expoID: child.val().expoID
          });
        });
      }).then(function () {
        var addUserID = true;
        for (i = 0; i < usersLiked.length; i++) {
          if (usersLiked[i].expoID == _names.expoID) {
            var addUserID = false;
          }
        }
        if (addUserID == true) {
          firebase.database().ref('posts/' + _this10.state.group + '/posts/' + _this10.state.passKey + '/commentlist/' + key + '/votes/' + _names.expoID).update({ voteNum: '0' });
          firebase.database().ref('posts/' + _this10.state.group + '/posts/' + _this10.state.passKey + '/commentlist/' + key + '/usersVoted').push().set({ expoID: _names.expoID });
        }
      }).then(function () {
        firebase.database().ref('posts/' + _this10.state.group + '/posts/' + _this10.state.passKey + '/commentlist/' + key + '/votes/' + _names.expoID).once("value", function (dataSnapshot) {
          voteNum = dataSnapshot.val().voteNum;
        }).then(function () {
          if (voteNum == 0) {
            firebase.database().ref('posts/' + _this10.state.group + '/posts/' + _this10.state.passKey + '/commentlist/' + key).update({ dislikes: dislikes + 1 });
            firebase.database().ref('posts/' + _this10.state.group + '/posts/' + _this10.state.passKey + '/commentlist/' + key + '/votes/' + _names.expoID).update({ voteNum: '1' });
            _this10.updateVotes(key, 'd');
          }
        });
      });
    }
