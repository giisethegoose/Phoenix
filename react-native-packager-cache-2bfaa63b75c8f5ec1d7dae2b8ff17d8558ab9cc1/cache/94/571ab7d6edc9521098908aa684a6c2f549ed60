            firebase.database().ref('posts/' + group + '/posts/' + key).update({ likes: likes + 1 });
            firebase.database().ref('posts/' + group + '/posts/' + key + '/votes/' + _names.expoID).update({ voteNum: '2' });
            _this12.setState({ postLikes: _this12.state.postLikes + 1 });
          }
        });
      });
    }
