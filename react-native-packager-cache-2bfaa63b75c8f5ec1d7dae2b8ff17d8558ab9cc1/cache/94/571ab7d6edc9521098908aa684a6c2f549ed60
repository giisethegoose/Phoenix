    key: 'pressLikeP',
    value: function pressLikeP(key, likes, dislikes, group) {
      var _this12 = this;

      var usersLiked = [];
      var query = firebase.database().ref('posts/' + group + '/posts/' + key + '/usersVoted').orderByKey();
      query.once('value', function (snap) {
        snap.forEach(function (child) {
          usersLiked.push({
            expoID: child.val().expoID
          });
        });
      }).then(function () {
        var addUserID = true;
        for (i = 0; i < usersLiked.length; i++) {
          if (usersLiked[i].expoID == _names.expoID) {
            var addUserID = false;
          }
        }
        if (addUserID == true) {
          firebase.database().ref('posts/' + group + '/posts/' + key + '/votes/' + _names.expoID).update({ voteNum: '0' });
          firebase.database().ref('posts/' + group + '/posts/' + key + '/usersVoted').push().set({ expoID: _names.expoID });
        }
      }).then(function () {
        firebase.database().ref('posts/' + group + '/posts/' + key + '/votes/' + _names.expoID).once("value", function (dataSnapshot) {
          voteNum = dataSnapshot.val().voteNum;
