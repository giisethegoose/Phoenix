    key: 'getItems',
    value: function getItems() {
      var _this36 = this;

      var items = [];
      firebase.database().ref('posts/' + this.state.group + '/posts').limitToLast(this.state.numOfPosts).orderByKey().once('value', function (snap) {
        snap.forEach(function (child) {
          items.push({
            content: child.val().content,
            key: child.key,
            nameID: child.val().userNameId,
            userID: child.val().userID,
            commentPreview: child.val().newestComment,
            comments: child.val().comments,
