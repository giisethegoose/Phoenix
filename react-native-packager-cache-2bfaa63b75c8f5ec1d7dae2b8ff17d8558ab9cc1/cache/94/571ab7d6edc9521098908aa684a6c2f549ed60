    value: function getItems() {
      var _this8 = this;

      this.setState({ passItems: [] });
      var query = firebase.database().ref('posts/' + this.state.group + '/posts/' + this.state.passKey + '/commentlist').orderByKey();
      query.once('value').then(function (snap) {
        var items = [];
        snap.forEach(function (child) {
          items.push({
            comment: child.val().comment,
            nameID: child.val().userNameId,
            userID: child.val().userID,
            likes: child.val().likes,
            dislikes: child.val().dislikes,
            key: child.key,
            time: child.val().time,
            replies: child.val().replies,
            indent: false
          });
        });
        console.log(items);
        for (i = 0; i < items.length; i++) {
          var timeStamp = 'error';
          var secondsBtwn = Math.floor((Date.now() - items[i].time) / 1000);
          if (secondsBtwn > 31536000) {
            timeStamp = Math.floor(secondsBtwn / 31536000) + 'y';
          } else if (secondsBtwn >= 604800) {
            timeStamp = Math.floor(secondsBtwn / 604800) + 'w';
          } else if (secondsBtwn >= 86400) {
            timeStamp = Math.floor(secondsBtwn / 86400) + 'd';
          } else if (secondsBtwn >= 3600) {
            timeStamp = Math.floor(secondsBtwn / 3600) + 'h';
          } else if (secondsBtwn >= 60) {
            timeStamp = Math.floor(secondsBtwn / 60) + 'm';
          } else {
            timeStamp = Math.floor(secondsBtwn) + 's';
          }
          items[i].time = timeStamp;
        }

        _this8.setState({ passItems: items });
      }).then(function () {

        var gotreplies = false;
        for (i = 0; i < _this8.state.passItems.length; i++) {
          if (_this8.state.passItems[i].replies == true) {
            _this8.getReplies(_this8.state.passItems[i].key, i);
            gotreplies = true;
          }
        }
        if (gotreplies == false) {
          _this8.setState({ firebaseItems: _this8.state.passItems });
        }
        _this8.setState({ refreshing: false });
        if (_this8.state.downloadedImage == false) {
          firebase.database().ref('posts/' + _this8.state.group + '/posts/' + _this8.state.passKey).once("value", function (dataSnapshot) {
            image = dataSnapshot.val().image;
          }).then(function () {
            _this8.setState({ bsfImage: image });
          });
          firebase.database().ref('posts/' + _this8.state.group + '/posts/' + _this8.state.passKey).once("value", function (dataSnapshot) {
            imageW = dataSnapshot.val().imageW;
          }).then(function () {
            _this8.setState({ imageW: imageW });
          });
          firebase.database().ref('posts/' + _this8.state.group + '/posts/' + _this8.state.passKey).once("value", function (dataSnapshot) {
            imageH = dataSnapshot.val().imageH;
          }).then(function () {
            _this8.setState({ imageH: imageH });
          });
          firebase.database().ref('posts/' + _this8.state.group + '/posts/' + _this8.state.passKey).once("value", function (dataSnapshot) {
            comments = dataSnapshot.val().comments;
          }).then(function () {
            _this8.setState({ numOfComments: comments });
          });
          firebase.database().ref('posts/' + _this8.state.group + '/posts/' + _this8.state.passKey).once("value", function (dataSnapshot) {
            likes = dataSnapshot.val().likes;
          }).then(function () {
            _this8.setState({ postLikes: likes });
          });
          firebase.database().ref('posts/' + _this8.state.group + '/posts/' + _this8.state.passKey).once("value", function (dataSnapshot) {
            dislikes = dataSnapshot.val().dislikes;
          }).then(function () {
            _this8.setState({ postDislikes: dislikes });
          });
          _this8.setState({ downloadedImage: true });
          firebase.database().ref('posts/' + _this8.state.group + '/posts/' + _this8.state.passKey).once("value", function (dataSnapshot) {
            timestamp = dataSnapshot.val().time;
          }).then(function () {
            var timeStamp = 'error';
            var secondsBtwn = Math.floor((Date.now() - timestamp) / 1000);
            if (secondsBtwn > 31536000) {
              timeStamp = Math.floor(secondsBtwn / 31536000) + 'y';
            } else if (secondsBtwn >= 604800) {
              timeStamp = Math.floor(secondsBtwn / 604800) + 'w';
            } else if (secondsBtwn >= 86400) {
              timeStamp = Math.floor(secondsBtwn / 86400) + 'd';
            } else if (secondsBtwn >= 3600) {
              timeStamp = Math.floor(secondsBtwn / 3600) + 'h';
            } else if (secondsBtwn >= 60) {
              timeStamp = Math.floor(secondsBtwn / 60) + 'm';
            } else {
              timeStamp = Math.floor(secondsBtwn) + 's';
            }
            _this8.state.time = timeStamp;
          });
        }
      });
    }
  }, {
    key: 'getReplies',
    value: function getReplies(key, indexnum) {
      var _this9 = this;

      var query = firebase.database().ref('posts/' + this.state.group + '/posts/' + this.state.passKey + '/commentlist/' + key + '/replylist').orderByKey();
      query.once('value').then(function (snap) {
        var replies = [];
        snap.forEach(function (child) {
          replies.push({
            comment: child.val().comment,
            nameID: child.val().userNameId,
            userID: child.val().userID,
            likes: child.val().likes,
            dislikes: child.val().dislikes,
            key: child.key,
            time: child.val().time,
            indent: true,
            parentKey: child.val().parentKey
          });
        });

        var items = _this9.state.passItems;
        for (i = 0; i < replies.length; i++) {
          var timeStamp = 'error';
          var secondsBtwn = Math.floor((Date.now() - replies[i].time) / 1000);
          if (secondsBtwn > 31536000) {
            timeStamp = Math.floor(secondsBtwn / 31536000) + 'y';
          } else if (secondsBtwn >= 604800) {
            timeStamp = Math.floor(secondsBtwn / 604800) + 'w';
          } else if (secondsBtwn >= 86400) {
            timeStamp = Math.floor(secondsBtwn / 86400) + 'd';
          } else if (secondsBtwn >= 3600) {
            timeStamp = Math.floor(secondsBtwn / 3600) + 'h';
          } else if (secondsBtwn >= 60) {
            timeStamp = Math.floor(secondsBtwn / 60) + 'm';
          } else {
            timeStamp = Math.floor(secondsBtwn) + 's';
          }
          replies[i].time = timeStamp;
        }

        replies.reverse();
        for (i = 0; i < replies.length; i++) {
          for (k = 0; k < items.length; k++) {
            if (replies[i].parentKey == items[k].key) {
              items.splice(k + 1, 0, replies[i]);
            }
          }
        }
        _this9.setState({ passItems: items });
        _this9.setState({ firebaseItems: items });
      });
    }
